{
  "rewrites": [
    {
      "source": "/api/lottery/(.*)",
      "destination": "/api/unified"
    },
    {
      "source": "/api/admin/(.*)",
      "destination": "/api/unified"
    },
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/dist/index.html"
    }
  ]
}
